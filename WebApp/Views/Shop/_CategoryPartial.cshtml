@model IEnumerable<WebApp.Models.Shop.ShopCategoryVM>

<h3>Categories</h3>
    <a class="list-group-item list-group-item-action" href="/shop/index">All products</a>
@foreach (var item in Model) 
{
    <a class="list-group-item list-group-item-action" href="/shop/category/@item.Name">@item.Name</a>
}

